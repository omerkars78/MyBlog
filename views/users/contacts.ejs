<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head")  %> 
</head>
<body>
    
    <%- include("../partials/nav")  %> 

    <div class="container mt-3">
        <div class="row">
          
            <div class="col-md-9">
                <h1 class="h3 mb-3"> <%= title %> </h1>

                <% if (contacts.length == 0) { %>
                    <%- include("../partials/no-products")  %>
                <% } %>

                <% contacts.forEach(contact => { %>
                    <%- include("../partials/contact-card", contact)  %> 
                <% }) %>

                
                              
             
            </div>
        </div>
    </div>

</body>
</html>